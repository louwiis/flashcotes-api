    "dotenv": "^17.2.3",
    "execa": "^9.6.0",
    "fastify": "^5.6.1",
    "fastify-plugin": "^5.1.0",
    "fastify-type-provider-zod": "^4.0.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "prisma": "^6.17.1",
    "tsx": "^4.19.0",
    "typescript": "^5.6.2"
  }
